{
  "firestore": {
    "database": "(default)",
    "location": "us-west1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": [
    {
      "bucket": "newsletter-b104f.firebasestorage.app",
      "rules": "storage.rules"
    }
  ],
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ],
      "secretEnvironment": [
        "GMAIL_USER",
        "GMAIL_APP_PASSWORD",
        "ADMIN_EMAIL"
      ]
    }
  ]
}
